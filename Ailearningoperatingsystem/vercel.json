{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/auth/(.*)",
            "destination": "/api/auth/[...path]"
        },
        {
            "source": "/api/ai/(.*)",
            "destination": "/api/ai/[...path]"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}